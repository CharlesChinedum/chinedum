---
export interface Props {
  url: string;
  icon: string;
  title?: string;
}

const { url, icon, title } = Astro.props as Props;
---

<a href={url} target="_blank">
  <img
    src={title !== "LinkedIn"
      ? `https://cdn.simpleicons.org/${icon}/white`
      : icon}
    alt=""
    class="w-5"
  />
</a>
